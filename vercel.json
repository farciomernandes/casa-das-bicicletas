{
    "version": 2,
    "buildCommand": "sh vercel.sh",
    "builds": [
      {
        "src": "src/main.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["dist/**/*"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "dist/main.js",
        "methods": [
          "GET",
          "POST",
          "PUT",
          "DELETE"
        ]
      }
    ]
  }
  