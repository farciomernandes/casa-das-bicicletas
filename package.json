{
  "name": "casa-das-bicicletas",
  "version": "0.0.1",
  "description": "",
  "author": "Marcio Fernandes",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npx nest build -p ts-config-build.json",
    "build:digitalocean": "npm install --production=false && npm run build && rm -rf node_modules && npm install --production --frozen-lockfile",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "seed": "nest build && ts-node dist/infra/db/typeorm/seeds/seed.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node -r tsconfig-paths/register node_modules/typeorm/cli -d dist/infra/db/database.provider.js",
    "migration:create": "npx typeorm migration:create",
    "migration:generate": "npx typeorm migration:generate -d src/infra/db/database.provider.ts",
    "migration:latest": "nest build && npx typeorm migration:run -d dist/infra/db/database.provider.js",
    "migration:revert": "nest build && npx typeorm migration:revert -d dist/infra/db/database.provider.js"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/cheerio": "^0.22.35",
    "aws-sdk": "^2.1548.0",
    "aws-sdk-mock": "^5.9.0",
    "axios": "^1.6.8",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "colorette": "^2.0.20",
    "correios-brasil": "^3.0.6",
    "cron": "^3.1.7",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.1",
    "express-basic-auth": "^1.2.1",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.0.9",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.20",
    "typeorm-naming-strategies": "^4.1.0"
  },
